	llamar nueva_linea
fin

 funcion DiasVividos (entero year, entero mes, entero dia, entero yearact, entero mesact, entero diaact, entero edad, entero diavida) : entero
inicio
	llamar GenerarTitulo ("DIAS VIVIDOS")
	llamar nueva_linea
	escriba "Ingresar su año de nacimiento en numeros: "
	lea year
	llamar nueva_linea
	escriba "Ingresar su mes de nacimiento en numeros: "
	lea mes
	llamar nueva_linea
	escriba "Ingresar su dia de nacimiento en numeros: "
	lea dia
	llamar nueva_linea
	escriba "Ingresar el año actual en numeros: "
	lea yearact
	llamar nueva_linea
	escriba "Ingresar el mes actual en numeros: "
	lea mesact
	llamar nueva_linea
	escriba "Ingresar el dia actial en numeros: "
	lea diaact
	llamar nueva_linea
	edad <- yearact - year
	diavida <- diaact+(mesact*30)+(edad*365)
	escriba "Sus dias vividos son: ",diavida,"."
	llamar nueva_linea
fin

