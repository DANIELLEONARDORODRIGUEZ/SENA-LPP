					llamar nueva_linea
					escriba "Nota final ",promedio
					llamar nueva_linea
					si promedio >=6 entonces
						escriba "APROBADO"
						llamar nueva_linea
					sino
						escriba "REPROBADO"
						llamar nueva_linea
					fin si
					contador <- contador +1
				fin mientras
			4:		
				escriba "SALIR"
				llamar nueva_linea
		sino: escriba "Opcion eqquivocada"
		llamar nueva_linea
		fin caso
	fin mientras
fin

 procedimiento ventas_almacen (entero j, entero i, entero dias )
inicio
	llamar GenerarTitulo ("VENTAS ALMACEN")
	gran_total <- 0
	escriba "Digite el numero de dias que se efectua la compra "
	lea dias
	i <- 1
	llamar nueva_linea
	para j <- 1 hasta dias haga
		totalV[j] <-0
		hora <- 8
		escriba "Dias: ",j
		mientras (hora <= 18) haga
			llamar nueva_linea
			escriba "Digite la hora "
			lea hora 
			si hora <=18 entonces
				escriba "Nombre del cliente "
				lea nombreV[i]
				llamar nueva_linea
				escriba "Ingresa el nombre del producto "
				lea tipoM [i]
				llamar nueva_linea
				escriba "Ingresar la cantidad "
				lea cantidad [i]
				llamar nueva_linea
				venta [i] <- precio [i]*cantidad[1]
				escriba "El comprador: ", nombreV[i]," compro: ", venta[i]," pesos."
				i <- i+1
				totalV[j] <- totalV[j] + venta[i-1]
			sino
			fin si
		fin mientras
		escriba "Lo sentimos, la tienda se cierra, son las 18 horas"
		llamar nueva_linea
		escriba "El total de ventas en el dia: ", j, " fue de: ", totalV[j]
	fin para
	para j <- 1 hasta dias haga
		llamar nueva_linea
		escriba "El total de ventasen el dia: ", j," fue de: ", totalV[j]
		gran_total <- gran_total + totalV[j]
	fin para
	llamar nueva_linea
	escriba"El total de venta los: ",dias," dias, fueron: ", gran_total, " pesos"
				
fin
