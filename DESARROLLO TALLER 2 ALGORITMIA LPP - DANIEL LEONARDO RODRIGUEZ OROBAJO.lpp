 funcion IVA16 (real productoNeto, real iva, real valor  ): real
inicio
	llamar GenerarTitulo ("CALCULAR EL IVA 16%")
	llamar nueva_linea
	escriba "Ingrese el valor del producto: "
	lea productoNeto
	iva <- (productoNeto * 16)/100
	valor <- productoNeto + iva
	llamar nueva_linea
	escriba "El valor del producto es: ",productoNeto
	llamar nueva_linea
	escriba "El valor del IVA 16% de este producto es: ",iva
	llamar nueva_linea
	escriba " El total de valor con IVA es: ",valor
fin
 funcion porcentaje (real numero, real porcentaje) : real
inicio
