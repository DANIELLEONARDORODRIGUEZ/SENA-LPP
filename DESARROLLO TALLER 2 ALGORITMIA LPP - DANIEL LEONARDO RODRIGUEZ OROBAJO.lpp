	llamar nueva_linea
	escriba "El resultado de la suma es: ",suma3
	llamar nueva_linea
fin

 funcion CalcularEdad (entero nacimiento) : entero
inicio
	llamar GenerarTitulo ("CALCULAR EDAD")
	llamar nueva_linea
