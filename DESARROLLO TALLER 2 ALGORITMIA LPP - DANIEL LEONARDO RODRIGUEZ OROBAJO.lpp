 procedimiento matriz (entero i, entero j)

inicio
	llamar GenerarTitulo("MATRIZ")
	llamar inicializar_Aleatorio ()
	para i <- 1 hasta 5 haga
		para j <- 1 hasta 5 haga
			mat[i,j] <- aleatorio()*11
		fin para
	fin para
	para i <- 1 hasta 5 haga
		escriba mat[i,1]," ",mat[i,2]," ",mat[i,3]," ",mat[i,4]," ",mat[i,5]
	fin para
fin

 procedimiento menupromedio (entero opcion35 , entero contador)
inicio
	llamar GenerarTitulo ("MENU PROMEDIO")
