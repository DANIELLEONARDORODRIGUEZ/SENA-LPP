 procedimiento NumeroMayorYMenor (real n1)
inicio
	
	n1<-20
	n2<-30
	n3<-10

	si (n1<n2)y(n2<n3) entonces
		llamar nueva_linea
		escriba "El numero mayor es: ",n3
		llamar nueva_linea
		escriba "El numero medio es: ",n2
		llamar nueva_linea
		escriba "El numero menor es: ",n1
	sino
		si (n1>n2)y(n2>n3) entonces
			llamar nueva_linea
			escriba "El numero mayor es: ",n1
			llamar nueva_linea
			escriba "El numero medio es: ",n2
			llamar nueva_linea
			escriba "El numero menor es: ",n3
		sino
			si (n1>n2)y(n2<n3) entonces
				llamar nueva_linea
				escriba "El numero mayor es: ",n3
				llamar nueva_linea
				escriba "El numero medio es: ",n2
				llamar nueva_linea
				escriba "El numero medio es: ",n1
			sino
				si (n1<n2)y(n2>n3)y(n1<n3) entonces
					llamar nueva_linea
					escriba "El numero mayor es: ",n2
					llamar nueva_linea
					escriba "El numero medio es: ",n3
					llamar nueva_linea
					escriba "El numero menor es: ",n1
				sino
				
				fin si
			fin si
		fin si
	fin si
fin
//OPERACIONES
 funcion AreaTriangulo (real base, real altura) : real
inicio
	llamar GenerarTitulo ("AREA DE UN TRIANGULO")
	llamar nueva_linea
	escriba "Ingresar la medida de la base en numeros: "
	lea base
	llamar nueva_linea
	escriba "Ingresar la medida de la altura en numeros: "
	lea altura
	areatriangulo <- (base*altura)/2
	llamar nueva_linea
	escriba "El area es: ",Areatriangulo,"cm."
	llamar nueva_linea
fin

 funcion PerimetroRectangulo (real base, real altura) : real
inicio
	llamar GenerarTitulo ("PERIMETRO DE UN RECTANGULO")
	llamar nueva_linea
	escriba "Ingresar la medida de la base en numeros: "
	lea base
	llamar nueva_linea
	escriba "Ingresar la medida de la altura en numeros: "
	lea altura
	perimetrorectangulo <- 2*(base+altura)
	llamar nueva_linea
	escriba "El perimetro es: ",perimetrorectangulo,"cm."
	llamar nueva_linea
fin	

 funcion Suma3Numeros (real num1, real num2, real num3) : real
inicio
	llamar GenerarTitulo ("SUMA DE 3 NUMEROS")
