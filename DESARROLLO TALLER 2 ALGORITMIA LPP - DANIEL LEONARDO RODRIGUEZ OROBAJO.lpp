fin
 funcion PorcentajePersonas (real hombre, real mujer) : real
inicio
	llamar GenerarTitulo ("CALCULO PORCENTAJE DE HOMBRES Y MUJERES")
	llamar nueva_linea
	escriba "Ingrese la cantidad de hombres que hay en numero: "
	lea hombre
	llamar nueva_linea
	escriba "Ingrese la cantidad de mujeres que hay en numero: "
	lea mujer
	total <- hombre+mujer
	porcentajeh <- (hombre *100)/total
	porcentajem <- (mujer *100)/total
	llamar nueva_linea
	escriba "son ",hombre," hombres y equivalen a: ",porcentajeh,"%"
	llamar nueva_linea
	escriba "son ",mujer," mujeres y equivalen a: ",porcentajem,"%"
	llamar nueva_linea
fin
